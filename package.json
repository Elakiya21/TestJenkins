  "scripts": {
    "sass-watcher-common": "./node_modules/.bin/node-sass --watch --recursive src/Common -o src/Common & SASS_WATCHER_COMMON_PID=$!",
    "sass-compile-common": "./node_modules/.bin/node-sass --recursive src/Common -o src/Common",
    "sass-watcher-styles": "./node_modules/.bin/node-sass --watch --recursive src/styles -o src/styles & SASS_WATCHER_STYLES_PID=$!",
    "sass-compile-styles": "./node_modules/.bin/node-sass --recursive src/styles -o src/styles",
    "sass-watcher-app": "./node_modules/.bin/node-sass --watch --recursive src/App -o src/App & SASS_WATCHER_APP_PID=$!",
    "sass-compile-app": "./node_modules/.bin/node-sass --recursive src/App -o src/App",
    "sass-watcher-store-front": "./node_modules/.bin/node-sass --watch --recursive src/Auth -o src/Auth & SASS_WATCHER_STORE_FRONT_PID=$!",
    "sass-compile-store-front": "./node_modules/.bin/node-sass --recursive src/Auth -o src/Auth",
    "sass-compile-common-prod": "./node_modules/.bin/node-sass --recursive src/Common -o src/Common --output-style compressed",
    "sass-compile-store-front-prod": "./node_modules/.bin/node-sass --recursive src/Auth -o src/Auth --output-style compressed",
    "sass-compile-styles-prod": "./node_modules/.bin/node-sass --recursive src/styles -o src/styles --output-style compressed",
    "sass-compile-app-prod": "./node_modules/.bin/node-sass --recursive src/App -o src/App --output-style compressed",
    "kill-pids": "kill $SASS_WATCHER_COMMON_PID && kill $SASS_WATCHER_STYLES_PID && kill $SASS_WATCHER_APP_PID && KILL $SASS_WATCHER_STORE_FRONT_PID",
    "compile-css": "npm run sass-compile-common && npm run sass-compile-styles && npm run sass-watcher-common && npm run sass-compile-store-front && npm run sass-watcher-store-front && npm run sass-watcher-styles && npm run sass-watcher-app && npm run sass-compile-app",
    "compile-node": "NODE_ENV=development webpack --config ./config/webpack.config.dev.js -w",
    "compile-css-prod": "npm run sass-compile-common-prod && npm run sass-compile-store-front-prod && npm run sass-compile-styles-prod && npm run sass-compile-app-prod",
    "compile-node-prod": "NODE_ENV=production node ./script/build.js",
    "build-dev": "npm run compile-css && npm run compile-node && npm run kill-pids",
    "start-dev": "NODE_ENV=development pm2 start ./config/pm2.dev.config.js --no-daemon",
    "production": "NODE_ENV=production pm2 start ./config/pm2.prod.config.js --no-daemon",
    "build-prod": "npm run compile-css-prod && npm run compile-node-prod"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "sideEffects": false,
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  }
}
